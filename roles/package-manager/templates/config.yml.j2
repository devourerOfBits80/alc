backup:
  enabled: true
  mode: incremental
  type: rsync
download:
  icons: true
  multithreaded: true
gems:
{% if FLATPAK_SUPPORTED | default(True) %}- flatpak
{% endif %}- arch
- appimage
- web
locale: en
memory_cache:
  data_expiration: 3600
  icon_expiration: 600
store_root_password: true
suggestions:
  by_type: 10
  enabled: true
system:
  notifications: true
  single_dependency_checking: false
ui:
  auto_scale: true
  hdpi: false
  scale_factor: 1.0
  style: breeze
  table:
    max_displayed: 100
updates:
  ask_for_reboot: true
  check_interval: 21600
